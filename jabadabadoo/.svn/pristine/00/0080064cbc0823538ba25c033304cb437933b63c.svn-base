{if:level=7}
<div class="booking">
    <p class="select-info">termin:</p>
    <div class="container-fluid">
    <select class="form-control mySelect" role="tablist" id="myTab">
    {loop:menu}
     <option value="date{__index__}" {if:first_child}class="selected"{endif:first_child}>{d_from} - {d_to}</option>
    {endloop:menu}
    </select>
    </div>
    
    
    <div class="tab-content">
    {loop:menu}    
    <div id="date{__index__}" class="tab-pane date{__index__}">
     
    <div class="container-fluid">
     
     <table class="table table-responsive table-condensed">
   <tbody>
    {if:price}
    <tr>
     <td>Cena:</td>
     <td>{price} PLN</td>
    </tr>
    {endif:price}
    {if:days}
   <tr>
     <td>Ilość dni:</td>
     <td>{days}</td>
   </tr>
   {endif:days}
    {if:group}
   <tr>
     <td>Grupa:</td>
     <td>{group} osób</td>
   </tr>
   {endif:group}
   {if:from}
    <tr>
     <td>Wylot:</td>
     <td>Berlin</td>
   </tr>
   {endif:from}
   {if:care}
    <tr>
     <td>Opieka:</td>
     <td><span class="{care}"></span></td>
   </tr>
    {endif:care}

 </tbody></table>
     
    </div>
      <a href="{href}" class="btn">Zapytaj</a>
    
    </div> 
    {endloop:menu}
    </div>
    {if:plain}<div class="price-info">{plain}</div>{endif:plain}
    
    
    <script>
    jQueryKam(function ($) {
    $('.mySelect').change(function(){
	var selected = $(this).find(':selected');
	 $('.optionvalue').fadeOut(function(){
         $('.optionvalue').html(selected.html()).fadeIn()
        .attr('class', 'optionvalue '+selected.val());   
	});
	
	var count = $('.booking .tab-pane').length;
	    $('.booking .tab-pane').slideUp(function(){
	        if(!--count) {
	        $('.'+selected.val()).slideDown();
	    }
	});
    });
    //$('.mySelect').val('date2');
    alert($('#date1'));
    $('#date1').css("display","block");
    });
    </script>
</div>
 {define:level_found=1}
{endif:level=7}

