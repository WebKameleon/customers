{if:level=11}
<ul class="nav nav-tabs nav-stacked" role="tablist">
  {loop:menu}
  <li {if:active} class="active"{endif:active}><a href="{href}" role="tab" data-toggle="tab">{alt}</a></li>
  {endloop:menu}
</ul>
