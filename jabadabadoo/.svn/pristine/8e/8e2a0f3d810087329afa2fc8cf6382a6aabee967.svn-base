{if:level=7}
<div class="booking">
    <p class="select-info">termin:</p>
    <div class="container-fluid">
    <select class="form-control mySelect" role="tablist" id="myTab">
    {loop:menu}
     <option value="date{__index__}" {if:first_child}class="selected"{endif:first_child}>{alt}</option>
    {endloop:menu}
    </select>
    </div>
    
    
    <div class="tab-content">
    {loop:menu}    
    <div class="tab-pane date{__index__}">
    
     <a href="{href}" class="btn">Zapytaj</a>
     
    <div class="container-fluid">
     {if:price}<div class="days">Cena: {price}</div>{endif:price}
     {if:days}<div class="days">Ilość dni: {days}</div>{endif:days}
     {if:group}<div class="group">Grupa: {group} osób</div>{endif:group}
     {if:from}<div class="from">Wylot: {from}</div>{endif:from}
     {if:care}<div class="care">Opieka:<div class="{care}"></div></div>{endif:care}
    </div>
    </div> 
    {endloop:menu}
    </div>
    
    {if:plain}<div class="price-info">{plain}</div>{endif:plain}
    
    <script>
$('.mySelect').change(function(){
    var selected = $(this).find(':selected');
    $('.optionvalue').fadeIn(function(){
         $('.optionvalue').html(selected.html()).fadeIn()
             .attr('class', 'optionvalue '+selected.val());   
    });
    var count = $('.booking .tab-pane').length;
    $('.booking .tab-pane').fadeIn(function(){
        if(!--count) {
           $('.'+selected.val()).fadeOut();
        }
	
    });
    
});
    </script>
</div>
 {define:level_found=1}
{endif:level=7}

